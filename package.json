{
  "name": "nexusone",
  "version": "2.0.1",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "node build-android.js",
    "build:android-debug": "expo run:android --variant debug",
    "build:android-release": "expo run:android --variant release",
    "build:android-eas": "eas build --platform android --profile development",
    "metro-start": "npx react-native start --reset-cache",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res --reset-cache",
    "clean": "npm cache clean --force && npx expo start --clear && cd android && ./gradlew clean && cd ..",
    "setup": "npm install && npx expo install --fix"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/slider": "^5.1.2",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.12",
    "@sentry/react-native": "~6.14.0",
    "axios": "^1.12.2",
    "expo": "~53.0.25",
    "expo-application": "~6.1.5",
    "expo-asset": "~11.1.7",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.8",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.1",
    "expo-image-picker": "~16.1.4",
    "expo-intent-launcher": "~12.1.5",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.5",
    "expo-notifications": "^0.31.4",
    "expo-print": "~14.1.4",
    "expo-secure-store": "~14.2.4",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "jwt-decode": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "13.13.5",
    "xlsx": "^0.18.5",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "dotenv": "^16.4.5",
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^15.0.0",
    "@types/react": "~19.0.10",
    "metro": "^0.82.5",
    "metro-config": "^0.82.5",
    "metro-resolver": "^0.82.5",
    "react-native-svg-transformer": "^1.5.1",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-chart-kit",
          "react-native-vector-icons"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "overrides": {
    "baseline-browser-mapping": "^2.9.0"
  },
  "private": true
}
